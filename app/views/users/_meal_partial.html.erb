        <h3><%= i.dish.category %> @ <%= i.restaurant.name %> | <%= i.rating %> <span class:"text">&#9733;</span> | <%= i.created_at.strftime("%m/%d/%Y %l:%M %P")%> | <%= link_to image_tag('trash.png'), i, method: :delete %></h3>
        <div>
          <p><%= i.review %></p>
        </div>


  <li id="idea_<%= idea.id %>" class="idea_list_item" style="display: none;">
