<h2 align="center"><%= link_to meal.dish.category, dish_url(meal.dish.id) %> @ <%= link_to meal.restaurant.name, restaurant_url(meal.restaurant.id) %></h2>
<div class="row">
  <div class="span2 offset4"><%= image_tag(meal.picture_url)%></div>
  <div class="span3"><p id="meal_partial"><div class='reviewstars' style="width:<%= 17*meal.rating.to_i %>px"></div>
  <% meal.rating.to_i.times do |i|%>
    <div role="text" aria-label class="star-rating rater-0 star star-rating-applied star-rating-live star-rating-hover" id="star"><a title="star">star</a></div>
  <% end %></p> <br>
  <p id="meal_partial"><%= meal.review %></p>
  <p></p>
<p id="meal_partial"><%= image_tag(meal.user.avatar.thumb.url) %> <%= meal.user.username %> | <%= meal.created_at.strftime("%m/%d/%Y %l:%M %P")%></p>


