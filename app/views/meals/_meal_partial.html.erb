<li id= "meal_<%= meal.id %>" class="meal">
    <% if meal.picture? %>
    <p class="meal_top_<%= meal.id %>" id="pic">
    <%= image_tag(meal.picture_url)%></p>
    <p class="meal_top_<%= meal.id %>" id="hidden"><%= meal.review %>
    </p>
    <% else %>
    <p class="meal_top_<%= meal.id %>" id="review">
    <%= meal.review %>
    </p>
    <% end %>
  <h3 class="meal">
    <%= meal.dish.category %> @ <%= meal.restaurant.name %><br>
        <div id="ratingstars">
          <% meal.rating.to_i.times do |i|%>
          <div role="text" aria-label class="star-rating rater-0 star star-rating-applied star-rating-live star-rating-hover" id="star"><a title="star">star</a></div>
          <% end %>
        </div>
        <!--     <%= meal.created_at.strftime("%m/%d/%Y %l:%M %P")%> |-->

  </h3>
</li>

